THREAD 񏈗()

ACW(GETID("MapleStory","MapleStoryClass"),1,1)

PET = 0

SLEEP(2)

While true

	KBD(VK_G,0,1000)
	SLEEP(2)
	KBD(VK_U,0,1000)
	KBD(VK_H,0,1000)

	FOR IDOU = 1 TO 20
		KBD(VK_SPACE,0,4000)
		SLEEP(1)
		IF CHKIMG("Γ_.BMP",0,0,0,800,600) THEN EXITEXIT
		KBD(VK_DELETE,0,500)
		IF CHKIMG("picture/PHS.bmp",0,585,515,800,600) THEN KBD(VK_5,0,1000)
	NEXT
	
	PET = PET + 1
	
	IFB PET > 2
		KBD(VK_T,0,1000)
		PET = 0
	ENDIF
wend

PROCEDURE 񏈗()
WHILE TRUE
//񏈗START

IFB CHKIMG("Ή.BMP",0,0,0,800,600)
	FOR X = 1 TO 20
		KBD(VK_LEFT,0,100)
		KBD(VK_RIGHT,0,100)
	NEXT
ENDIF

//񏈗END
