THREAD 並列処理()

ACW(GETID("MapleStory","MapleStoryClass"),1,1)

PET = 0

SLEEP(2)

While true
	KBD(VK_G,0,1000)
	SLEEP(2)
	KBD(VK_U,0,1000)
	KBD(VK_H,0,1000)

	FOR IDOU = 1 TO 20
		for X = 1 to 20 
			KBD(VK_X,0,100)
		next
		KBD(VK_SPACE,0,800)
		KBD(VK_SPACE,0,250)
		SLEEP(0.5)
		IF CHKIMG("緑点.BMP",0,0,0,800,600) THEN EXITEXIT
		KBD(VK_DELETE,0,500)
		IF CHKIMG("picture/PHS.bmp",0,585,515,800,600) THEN KBD(VK_5,0,1000)
		recovery()
		KBD(VK_2,0,500)

		ifb chkimg("./picture/赤点.BMP",0,0,0,800,600)
			SLEEP(10)
		endif

		// 丼イベここから*/
		ifb CHKIMG("./picture/プレゼント.BMP",0,0,0,800,600)
			BTN(LEFT,CLICK,G_IMG_X,G_IMG_Y)
			BTN(LEFT,CLICK,200,200,500)
		endif
		IF CHKIMG("次へ.BMP",0,0,0,800,600) THEN BTN(LEFT,CLICK,G_IMG_X,G_IMG_Y)
		// 丼イベここまで*/
	NEXT
	
	PET = PET + 1
	
	IFB PET > 2
		KBD(VK_T,0,1000)
		PET = 0
	ENDIF
wend

PROCEDURE 並列処理()
WHILE TRUE
//並列処理START

IFB CHKIMG("石化.BMP",0,0,0,800,600)
	FOR X = 1 TO 20
		KBD(VK_LEFT,0,100)
		KBD(VK_RIGHT,0,100)
	NEXT
ENDIF



//並列処理END
WEND

FEND

PROCEDURE recovery()

		IFB CHKIMG("雪女.bmp",0,585,515,800,600)
			KBD(VK_U,0,500)
			KBD(VK_9,0,800)
			EXIT
		ENDIF

		KBD(VK_HOME,0,800)

		KBD(VK_R,0,500)

FEND