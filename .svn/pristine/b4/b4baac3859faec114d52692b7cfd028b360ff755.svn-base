ACW(GETID("MapleStory","MapleStoryClass"),1,1)

	THREAD 並列処理()

	//鬼神、ペットの餌、はしごつかみ判定変数
	//TODO:GETTIME使うかも
	kishinCounter    = 0
	petCounter       = 0
	notBattleCounter = 0

while true

	KBD(VK_T,0,1000)
	KBD(VK_G,0,1000)
	KBD(VK_H,0,3000)
	KBD(VK_CTRL,0,1000)

	sleep(1)

	for attack = 1 to 20
		IF CHKIMG("picture/PHS.bmp",0,585,515,800,600) THEN KBD(VK_5,0,1000)
		for skill = 1 to 6
			KBD(VK_A,0,700)
			//ロープ判定
			IFB CHKIMG("黄点.BMP",0,80,113,87,130)
				KBD(VK_UP,1,500)
				KBD(VK_UP,2,3000)
				KBD(VK_RIGHT,1,300)
				KBD(VK_RIGHT,2,500)
				notBattleCounter = 0
			ENDIF
		next
		
		sleep(1)
		
		checkYelloPoint(notBattleCounter)
		
		sleep(0.5)
		
		recovery()
		
		kishinCounter = kishinCounter + 1
		petCounter    = petCounter    + 1
		
		IFB kishinCounter > 5
			KBD(VK_H,0,500)
			kishinCounter = 0
		ENDIF
		
		IFB petCounter > 40
			KBD(VK_T,0,1000)
			petCounter = 0
		ENDIF

		IF CHKIMG("突発.BMP",0,0,0,800,600) THEN BTN(LEFT,CLICK,G_IMG_X,G_IMG_Y)		
		IF CHKIMG("緑点.BMP",0,0,0,800,600) THEN EXITEXIT

	next

wend

PROCEDURE 並列処理()
WHILE TRUE

	IFB CHKIMG("石化.BMP",0,0,0,800,600)
		FOR X = 1 TO 20
			KBD(VK_LEFT,0,100)
			KBD(VK_RIGHT,0,100)
		NEXT
	ENDIF

	IFB CHKIMG("赤点.BMP",0,0,0,800,600)
		FOR BEEPS = 1 to 10
			SOUND("E:\jubeat_analyser150530\fumen\bgm\Lisa-RICCIA.mp3")
			sleep(0.2)
		NEXT
	ENDIF



WEND
FEND

PROCEDURE recovery()

	IFB CHKIMG("霊力回復.BMP",0,590,500,800,600)

		IFB CHKIMG("雪女.bmp",0,585,515,800,600)
			KBD(VK_U,0,500)
			KBD(VK_9,0,800)
			EXIT
		ENDIF


		IFB CHKIMG("白狐.bmp",0,585,515,800,600)
			//KBD(VK_INSERT,0,100)
			KBD(VK_U,0,800)
			//KBD(VK_INSERT,0,500)
			KBD(VK_CTRL,0,500)
			EXIT
		ENDIF

		IFB CHKIMG("妖怪巻.bmp",0,585,515,800,600)
			//KBD(VK_INSERT,0,100)
			KBD(VK_U,0,800)
			//KBD(VK_INSERT,0,500)
			KBD(VK_SHIFT,0,500)
			EXIT
		ENDIF

		//KBD(VK_INSERT,0,300)
		KBD(VK_U,0,500)
		//KBD(VK_INSERT,0,500)
	ENDIF

FEND

function checkYelloPoint(notBattleCounter)

	//下段左にいた場合右に行く
	IFB CHKIMG("黄点.BMP",0,0,0,75,160)
		KBD(VK_RIGHT,1,500)
		KBD(VK_RIGHT,2,1500)
		EXIT
	ENDIF

	//下段右にいた場合左にいく
	IFB CHKIMG("黄点.BMP",0,90,85,140,135)
		KBD(VK_LEFT,1,500)
		KBD(VK_LEFT,2,1000)
		EXIT
	ENDIF
	
	//下段にいた場合上にいく
	IFB CHKIMG("黄点.BMP",0,66,126,105,140)
		KBD(VK_UP,1,700)
		KBD(VK_SPACE,0,500)
		KBD(VK_SPACE,0,300)
		KBD(VK_UP,2,600)
		EXIT
	ENDIF
	
	//中段にいた場合上にいく
	IFB CHKIMG("黄点.BMP",0,60,104,110,115)
		KBD(VK_UP,1,500)
		for i = 1 to 2
			KBD(VK_SPACE,0,700)
		next
		KBD(VK_UP,2,500)
		EXIT
	ENDIF
	
		
	//上段にいた場合下にいく
	IFB CHKIMG("黄点.BMP",0,0,0,111,105)
		KBD(VK_DOWN,1,500)
		FOR C = 1 to 2
			KBD(VK_C,0,500)
			KBD(VK_C,0,500)
		next
		KBD(VK_DOWN,2,200)
		KBD(VK_HOME,0,800)
		EXIT
	ENDIF
	
	
	notBattleCounter = notBattleCounter + 1
	
	IFB notBattleCounter > 3
		KBD(VK_LEFT,1,500)
		KBD(VK_LEFT,2,1500)
		notBattleCounter = 0
	ENDIF
	
	result = notBattleCounter 

FEND