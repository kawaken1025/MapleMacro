function coreBreak(x,y,rangeNum,plusX,plusY)

	NEXT_PAGE_X = 790
	NEXT_PAGE_Y = 612
	//BTN(LEFT,CLICK,NEXT_PAGE_X,NEXT_PAGE_Y,500)

	clickX = x
	clickY = y

	for i = 1 to rangeNum
		for RowClick = 1 to rangeNum
			BTN(LEFT,CLICK,clickX,clickY,200)
			clickX = clickX + plusX
		next
		clickX = x
		clickY = clickY + plusY
	next



	result = 0

fend