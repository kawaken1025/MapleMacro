CALL ../lib/MapleLibrary.uws;
lib.activeWindow()

THREAD 並列処理A() 

chkimg("../picture/黄点.bmp")
Xa = g_img_x
Ya = g_img_y

while true
    while true
        while chkimg("../picture/黄点.bmp",0,0,0,Xa-2,Ya+200)
            kbd(vk_right,1)
            sleep(0.04)
            kbd(vk_right,2)
        wend

        while !chkimg("../picture/黄点.bmp",0,0,0,Xa+2,Ya+200)
            kbd(vk_left,1)
            sleep(0.04)
            kbd(vk_left,2)
        wend

        sleep(0.1)

        IF chkimg("../picture/kishin.bmp") then break
        //▼▼GM対策▼▼
        ifb chkimg("../picture/GMresponse/ご返答.bmp",1,5,600,90,780) and !chkimg("../picture/ハート拡声器.bmp",0,5,600,400,780)
        //chkimg("../picture/redpoint.bmp",0,10,85,250,160) and _
            for SND = 1 to 20
                sound("beep")
                sleep(0.5)
                if chkimg("../picture/GMresponse/ご返答.bmp",1,55,600,420,780) then break
            next
            lib.checkGameMaster()
        endif

    wend

    ifb chkimg("../picture/黄点.bmp",0,0,0,Xa,Ya+200)
        while chkimg("../picture/黄点.bmp",0,0,0,Xa,Ya+200)
            kbd(vk_right,1)
            sleep(0.08)
            kbd(vk_right,2)
        wend

        sleep(0.5)
        kbd(VK_A,0,110)
        kbd(VK_A,0,150)
        kbd(VK_A,0,150)
        sleep(1.5)

        while chkimg("../picture/kishin.bmp")
            kbd(VK_H,0,210)
        wend

    elseif !chkimg("../picture/黄点.bmp",0,0,0,Xa,200)

        while !chkimg("../picture/黄点.bmp",0,0,0,Xa,200)
            kbd(vk_left,1)
            sleep(0.08)
            kbd(vk_left,2)
        wend

        sleep(0.5)
        kbd(VK_A,0,110)
        kbd(VK_A,0,150)
        kbd(VK_A,0,150)
        sleep(1.5)

        while chkimg("../picture/kishin.bmp",0,847,714,1025,792)
            kbd(VK_H,0,210)
        wend

    endif

wend



PROCEDURE 並列処理A()

	DIM list[] = 2,3,4
	WHILE TRUE
		For KK = 1 TO list[KK]
			sleep(0.5)
		NEXT
		kbd(vk_X,0,500)
	WEND
FEND